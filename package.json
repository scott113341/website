{
  "name": "website",
  "description": "Scott Hardy's personal website.",
  "author": "Scott Hardy",
  "repository": "https://github.com/scott113341/website.git",
  "scripts": {
    "develop": "nodemon --ext jade,less --ignore build/ --exec 'npm run build && npm run serve'",
    "serve": "http-server build/ -p 3000 -c 0",
    "build": "./build.sh",
    "deploy": "npm run build && push-dir --dir build --branch gh-pages --cleanup"
  },
  "devDependencies": {
    "git-credential-env": "1.0.0",
    "http-server": "0.8.5",
    "jade": "git://github.com/pugjs/jade.git#ea4be34ffe72af6ff39c7b750a06fed1e5bdca56",
    "jade-cli": "0.1.1",
    "jstransformer-marked": "1.0.1",
    "less": "2.6.0",
    "less-plugin-clean-css": "1.5.1",
    "nodemon": "1.8.1",
    "push-dir": "0.2.1"
  },
  "license": "MIT"
}
